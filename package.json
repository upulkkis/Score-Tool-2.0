{
  "name": "react-score-tool",
  "version": "1.0.0",
  "description": "A new version of score-tool app",
  "homepage": ".",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "react-scripts --max_old_space_size=8192 start",
    "build": "react-scripts --max_old_space_size=8192 build && compress-cra",
    "build2": "react-scripts build && gzip build/static/js/*.js && gzip build/static/css/*.css",
    "deploy": "scp -r ./build/* root@score-tool.com:/var/www/new.score-tool.com",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensheetmusicdisplay/react-opensheetmusicdisplay.git"
  },
  "keywords": [
    "orchestration",
    "sheet",
    "music"
  ],
  "author": "Uljas Pulkkis",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.2",
    "axios": "^0.22.0",
    "fuzzball": "^2.0.2",
    "opensheetmusicdisplay": "^1.0.0",
    "plotly.js": "^2.5.1",
    "react-drag-drop-container": "^6.1.1",
    "react-dropzone": "^11.4.2",
    "react-piano": "^3.1.3",
    "react-plotly.js": "^2.5.1",
    "save-svg-as-png": "^1.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "compress-create-react-app": "^1.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.2.0",
    "rewire": "^5.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
